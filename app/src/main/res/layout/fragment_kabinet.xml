<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color"
    android:orientation="vertical">
    <LinearLayout
        android:id="@+id/nameProfilLL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_14sdp"
        android:background="@drawable/circularbordersolid"
        android:gravity="center"
        android:padding="@dimen/_8sdp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="@dimen/_16sdp"
        android:layout_marginRight="@dimen/_16sdp">
        <com.rengwuxian.materialedittext.MaterialEditText
            android:id="@+id/nameEd_pr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@null"
            android:clickable="false"
            android:focusable="false"
            android:textCursorDrawable="@color/darkgrey"
            android:fontFamily="@font/proximanovaregular"
            app:met_floatingLabel="normal"
            android:hint="Ism"
            android:drawableRight="@drawable/ic_baseline_edit_24"
            android:inputType="text"
            android:textSize="@dimen/_12sdp"
             />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/emailProfilLL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_18sdp"
        android:background="@drawable/circularbordersolid"
        android:gravity="center"
        android:padding="@dimen/_8sdp"
        app:layout_constraintTop_toBottomOf="@id/nameProfilLL"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="@dimen/_16sdp"
        android:layout_marginRight="@dimen/_16sdp"
        >
        <com.rengwuxian.materialedittext.MaterialEditText
            android:id="@+id/emailEd_pr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@null"
            android:fontFamily="@font/proximanovaregular"
            app:met_floatingLabel="normal"
            android:hint="Email pochta"
            android:clickable="false"

            android:focusable="false"
            android:textCursorDrawable="@color/darkgrey"
            android:inputType="text"
            android:textSize="@dimen/_12sdp" />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/parolProfilLL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="@drawable/circularbordersolid"
        android:gravity="center"
        android:padding="@dimen/_8sdp"

        app:layout_constraintTop_toBottomOf="@id/emailProfilLL"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="@dimen/_16sdp"
        android:layout_marginRight="@dimen/_16sdp"
        >
        <com.rengwuxian.materialedittext.MaterialEditText
            android:id="@+id/parolEd_pr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@null"
            android:fontFamily="@font/proximanovaregular"
            app:met_floatingLabel="normal"
            android:hint="Parol"
            android:clickable="false"
            android:focusable="false"
            android:drawableRight="@drawable/ic_baseline_edit_24"
            android:inputType="text"
            android:textCursorDrawable="@color/darkgrey"
            android:textSize="@dimen/_12sdp" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/phoneProfilLL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_18sdp"
        android:background="@drawable/circularbordersolid"
        android:gravity="center"
        android:padding="@dimen/_8sdp"
        android:clickable="false"
        android:focusable="false"
        app:layout_constraintTop_toBottomOf="@id/parolProfilLL"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="@dimen/_16sdp"
        android:layout_marginRight="@dimen/_16sdp"
        >
        <com.rengwuxian.materialedittext.MaterialEditText
            android:id="@+id/phoneEd_pr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@null"
            android:fontFamily="@font/proximanovaregular"
            app:met_floatingLabel="normal"
            android:hint="Telefon raqam"
            android:clickable="true"
            android:cursorVisible="false"
            android:focusable="false"
            android:textCursorDrawable="@color/darkgrey"
            android:inputType="numberDecimal"
            android:textSize="@dimen/_12sdp" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/limitProfilLL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_18sdp"
        android:background="@drawable/circularbordersolid"
        android:gravity="center"
        android:padding="@dimen/_8sdp"
        app:layout_constraintTop_toBottomOf="@id/phoneProfilLL"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="@dimen/_16sdp"
        android:layout_marginRight="@dimen/_16sdp"
        >

        <com.rengwuxian.materialedittext.MaterialEditText
            android:id="@+id/limitEd_pr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@null"
            android:clickable="true"
            android:cursorVisible="false"
            android:drawableEnd="@drawable/ic_baseline_info_24"
            android:focusable="true"
            android:fontFamily="@font/proximanovaregular"
            android:hint="Limit qoldig'i"

            android:inputType="text"
            android:textSize="@dimen/_12sdp"
            app:met_floatingLabel="normal" />
    </LinearLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_save"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_16sdp"
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_marginEnd="@dimen/_10sdp"
        android:background="@drawable/circularbluebordersolid"
        android:fontFamily="@font/proximanovaregular"
        android:padding="@dimen/_8sdp"
        android:text="Tizimdan chiqish"
        android:textColor="@color/white"
        app:layout_constraintTop_toBottomOf="@id/limitProfilLL"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>














































